    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-standard": "^5.0.0",
    "jsdom": "^25.0.1",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "p-retry": "^6.2.0",
    "playwright": "^1.48.0",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "shx": "^0.3.4",
